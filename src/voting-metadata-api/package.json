{
  "name": "ipfs-cache-api",
  "version": "1.0.0",
  "description": "API to cache and store IPFS Files on S3 as well as assist in local development",
  "main": "src/lambda-handler.js",
  "author": "Algorand Foundation",
  "type": "commonjs",
  "scripts": {
    "dev": "npm run routes && ts-node-dev --transpile-only --watch .env -r dotenv/config src/server.ts",
    "build": "rimraf build && tsoa spec-and-routes && tsc && copyfiles package.json package-lock.json build && cd build && npm ci --omit=dev",
    "routes": "tsoa routes",
    "clean": "rimraf node_modules && rimraf build",
    "lint": "eslint --ext .ts,.tsx \"src/\"",
    "lint:fix": "eslint --ext .ts,.tsx \"src/\" --fix",
    "typecheck": "tsoa routes && tsc --noEmit",
    "format": "prettier --write .",
    "audit:resolve": "resolve-audit",
    "audit": "check-audit"
  },
  "dependencies": {
    "algosdk": "^2.2.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "glob": "^9.3.2",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "multiformats": "^9.9.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "sharp": "^0.31.3",
    "tsoa": "^5.1.1",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "web3.storage": "^4.5.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.299.0",
    "@aws-sdk/client-secrets-manager": "^3.299.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.3",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "aws-lambda": "^1.0.7",
    "aws-xray-sdk": "^3.4.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-audit-resolver": "^3.0.0-9",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
